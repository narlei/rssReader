# rssReader
platform :ios, '10.3'

target 'rssReader' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for rssReader

  # Menu
  pod 'FAPanels'

  # Helper
  pod 'R.swift'
  pod 'DateToolsSwift'

  # Request
  pod 'Alamofire'

  # XML
  pod 'AlamofireRSSParser'

  # Cache
  pod 'SDWebImage'

  target 'rssReaderTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
